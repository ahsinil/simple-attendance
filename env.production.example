# Laravel Attendance System - Production Environment

# App Settings
APP_KEY=base64:GENERATE_A_NEW_KEY_WITH_php_artisan_key:generate
APP_URL=https://your-domain.com

# Database
DB_DATABASE=simple_attendance
DB_USERNAME=attendance_user
DB_PASSWORD=your_secure_password_here
DB_ROOT_PASSWORD=your_root_password_here

# Barcode Security (generate a random 32+ character string)
BARCODE_SECRET_KEY=your_random_secret_key_here_minimum_32_chars
